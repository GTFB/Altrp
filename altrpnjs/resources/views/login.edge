<form  method="POST" action="{{ route('post.login') }}" class="flex-center">
  {{csrfField()}}
  <div class="contain">
    <label>
      <span class="label">Email</span>
      <input class="line"
             type="email"
             name="email"
             required
             autocomplete="email"
             autofocus>
    </label>
    <label>
      <span class="label mt-10">Password</span>
      <input class="line" type="password" name="password" required autocomplete="current-password" >
    </label>
    <div class="label mt-10">
      <label>
        <input type="checkbox"
               name="remember" id="remember"
        > Remember me</label>
      <button class="but">Log in</button>
    </div>
    <a href="#" class="label mt-10">Lost password?</a>
  </div>
</form>
