  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "front-start": "webpack-dev-server --config webpack.front.dev.js",
    "editor-start": "webpack-dev-server --config webpack.editor.dev.js",
    "admin-start": "webpack-dev-server --config webpack.admin.dev.js",
    "reports-start": "webpack-dev-server --config webpack.reports.dev.js",
    "front-build": "webpack --config webpack.front.prod.js",
    "editor-build": "webpack --config webpack.editor.prod.js",
    "admin-build": "webpack --config webpack.admin.prod.js",
    "reports-build": "webpack --config webpack.reports.prod.js",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "pack": "npm run front-build && npm run editor-build && npm run admin-build && gulp pack",
    "only-pack": "gulp pack",
    "run-all": "npm-run-all --parallel admin-start editor-start front-start",
    "reports:dev": "cd resources/modules/altrp-reports && PORT=3010 react-scripts start",
    "reports:build": "rm -rf public/modules/altrp-reports && cd resources/modules/altrp-reports && PUBLIC_URL=/modules/reports/ react-scripts build",
    "reports:move": "mv -f resources/modules/altrp-reports/build public/modules/reports",
    "docs": "jsdoc resources/modules -r"
  },
