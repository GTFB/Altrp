{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "beyondcode/laravel-websockets": "^1.9",
        "chrisjean/php-ico": "^1.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.5",
        "ixudra/curl": "^6.20",
        "jackiedo/dotenv-editor": "1.*",
        "jenssegers/date": "^3.2",
        "joshbrw/laravel-module-installer": "^1.0",
        "kint-php/kint": "^3.3",
        "laravel-notification-channels/telegram": "^0.5.1",
        "laravel/framework": "^6.2",
        "laravel/passport": "^8.4",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^1.2",
        "league/color-extractor": "0.3.*",
        "nwidart/laravel-modules": "^8.0",
        "phpoffice/phpspreadsheet": "^1.16",
        "phpoffice/phpword": "^0.18.2",
        "pulkitjalan/geoip": "^4.1",
        "pusher/pusher-php-server": "4.1.1",
        "salsify/json-streaming-parser": "^8.2",
        "santigarcor/laratrust": "5.2.*",
        "violet/streaming-json-encoder": "^1.1",
        "westacks/telebot": "^1.12"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laracasts/generators": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/functions.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Plugins\\": "Plugins",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Plugins"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "php artisan vendor:publish --tag=public --force"
        ],
        "post-install-cmd": [
            "php artisan vendor:publish --tag=public --force",
            "php artisan modules-statuses:write",
            "php artisan make:provider AltrpRepositoryServiceProvider",
            "php artisan make:provider AltrpProviders/AppServiceProvider",
            "php artisan provider:write App/Providers/AltrpProviders/AppServiceProvider",
            "php artisan provider:write App/Providers/AltrpRepositoryServiceProvider"
        ]
    }
}
