  <form class="contain-wide" id="altrp-install" method="POST" action="{{ route( 'altrp-install.post' ) }}">
    {{ csrfField() }}
    <div class="row-heading">
      General
    </div>
    <div class="row">
      <div class="col-1">
        <label>
          <span class="label">Site name</span>
          <input class="line" name="site_name" required >
        </label>
      </div>
    </div>
    <div class="row-heading">
      Database
    </div>
    <div class="row">
      <div class="col-1">
        <label>
          <span class="label">Username</span>
          <input class="line" name="database_username" required >
        </label>
        <label>
          <span class="label">Database name</span>
          <input class="line" name="database_name" required >
        </label>
      </div>
      <div class="col-2">
        <label>
          <span class="label">Password</span>
          <input class="line" name="database_password"  >
        </label>
      </div>
    </div>
    <div class="row-heading">
      Administrator
    </div>
    <div class="row">
      <div class="col-1">
        <label>
          <span class="label">Name</span>
          <input class="line" name="name" required>
        </label>
        <label>
          <span class="label">Email</span>
          <input class="line" name="email" required >
        </label>
      </div>
      <div class="col-2">
        <label>
          <span class="label">LastName</span>
          <input class="line" name="last_name" required >
        </label>
        <label>
          <span class="label">Password</span>
          <input class="line" type="password" name="password" required>
        </label>
      </div>
    </div>
    <div class="row-heading">
      <button class="but">Install</button>
    </div>
  </form>
</div>
